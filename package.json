{
  "name": "page-module",
  "version": "1.0.0",
  "description": "Page module that can be used to get sizes and parameters easily.",
  "main": "src/Page.js",
  "scripts": {
    "build": "browserify src/Page.js -t babelify --outfile dist/Page.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incentro/PageModule.git"
  },
  "keywords": [
    "page",
    "parameters",
    "height",
    "sizes"
  ],
  "author": "Kasper Reijnders <kasper.reijnders@incentro.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/incentro/PageModule/issues"
  },
  "homepage": "https://github.com/incentro/PageModule#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015-loose"
          ]
        }
      ]
    ]
  }
}
